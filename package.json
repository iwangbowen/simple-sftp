          "command": "simpleSftp.deleteHost",
          "when": "view == simpleSftp.hosts && viewItem == host",
          "group": "9_danger@1"
        },
        {
          "command": "simpleSftp.browseBookmarkWebview",
          "when": "view == simpleSftp.hosts && viewItem == bookmark",
          "group": "inline@1"
        },
        {
          "command": "simpleSftp.browseBookmark",
          "when": "view == simpleSftp.hosts && viewItem == bookmark",
          "group": "inline@2"
        },
        {
          "command": "simpleSftp.renameBookmark",
          "when": "view == simpleSftp.hosts && viewItem == bookmark",
          "group": "inline@3"
        },
        {
          "command": "simpleSftp.editBookmarkDescription",
          "when": "view == simpleSftp.hosts && viewItem == bookmark",
          "group": "1_edit@1"
        },
        {
          "command": "simpleSftp.deleteBookmark",
          "when": "view == simpleSftp.hosts && viewItem == bookmark",
          "group": "9_danger@1"
        },
        {
          "command": "simpleSftp.addHostToGroup",
          "when": "view == simpleSftp.hosts && viewItem == group",
          "group": "inline@1"
        },
        {
          "command": "simpleSftp.editGroup",
          "when": "view == simpleSftp.hosts && viewItem == group",
          "group": "inline@2"
        },
        {
          "command": "simpleSftp.exportGroup",
          "when": "view == simpleSftp.hosts && viewItem == group",
          "group": "1_export@1"
        },
        {
          "command": "simpleSftp.deleteHost",
          "when": "view == simpleSftp.hosts && viewItem == group",
          "group": "2_danger@1"
        },
        {
          "command": "simpleSftp.pauseTask",
          "when": "view == simpleSftp.transferQueue && viewItem =~ /status-running/",
          "group": "inline@1"
        },
        {
          "command": "simpleSftp.resumeTask",
          "when": "view == simpleSftp.transferQueue && viewItem =~ /status-paused/",
          "group": "inline@1"
        },
        {
          "command": "simpleSftp.cancelTask",
          "when": "view == simpleSftp.transferQueue && viewItem =~ /(status-running|status-pending|status-paused)/",
          "group": "inline@2"
        },
        {
          "command": "simpleSftp.retryTask",
          "when": "view == simpleSftp.transferQueue && viewItem =~ /status-failed/",
          "group": "1_control@1"
        },
        {
          "command": "simpleSftp.showTaskDetails",
          "when": "view == simpleSftp.transferQueue",
          "group": "2_info@1"
        },
        {
          "command": "simpleSftp.removeTask",
          "when": "view == simpleSftp.transferQueue",
          "group": "9_danger@1"
        },
        {
          "command": "simpleSftp.removeHistoryTask",
          "when": "view == simpleSftp.transferHistory && viewItem =~ /historyTask/",
          "group": "inline@1"
        }
      ],
      "explorer/context": [
        {
          "command": "simpleSftp.uploadFile",
          "group": "2_workspace@1"
        },
        {
          "command": "simpleSftp.downloadToLocal",
          "group": "2_workspace@2"
        }
      ],
      "editor/context": [
        {
          "command": "simpleSftp.uploadFile",
          "group": "2_workspace@1"
        },
        {
          "command": "simpleSftp.downloadToLocal",
          "group": "2_workspace@2"
        }
      ]
    },
    "configuration": {
      "title": "Simple SFTP",
      "properties": {
        "simpleSftp.showDotFiles": {
          "type": "boolean",
          "default": true,
          "description": "Show hidden files and directories (dot files) when browsing remote files"
        },
        "simpleSftp.speedUnit": {
          "type": "string",
          "enum": [
            "auto",
            "KB",
            "MB"
          ],
          "default": "auto",
          "description": "Download speed display unit. 'auto' will automatically choose between KB/s and MB/s based on speed"
        },
        "simpleSftp.transferQueue.maxConcurrent": {
          "type": "number",
          "default": 2,
          "minimum": 1,
          "maximum": 10,
          "description": "Maximum number of concurrent file transfers"
        },
        "simpleSftp.transferQueue.autoRetry": {
          "type": "boolean",
          "default": true,
          "description": "Automatically retry failed transfers"
        },
        "simpleSftp.transferQueue.maxRetries": {
          "type": "number",
          "default": 3,
          "minimum": 0,
          "maximum": 10,
          "description": "Maximum number of retry attempts for failed transfers"
        },
        "simpleSftp.transferQueue.retryDelay": {
          "type": "number",
          "default": 2000,
          "minimum": 1000,
          "maximum": 60000,
          "description": "Delay between retry attempts (in milliseconds)"
        },
        "simpleSftp.transferQueue.showNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Show notifications for transfer completion"
        },
        "simpleSftp.transferQueue.historySize": {
          "type": "number",
          "default": 100,
          "minimum": 10,
          "maximum": 1000,
          "description": "Maximum number of transfer history records to keep"
        },
        "simpleSftp.verification.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Verify file integrity after transfer using checksum. Requires md5sum/sha256sum tools on the remote server. Recommended for critical file transfers."
        },
        "simpleSftp.verification.algorithm": {
          "type": "string",
          "enum": [
            "md5",
            "sha256"
          ],
          "default": "sha256",
          "description": "Checksum algorithm to use for file verification. MD5 is faster but less secure. SHA256 is more secure but slower."
        },
        "simpleSftp.verification.threshold": {
          "type": "number",
          "default": 10,
          "minimum": 0,
          "description": "Minimum file size (in MB) for checksum verification. Default: 10 MB. Set to 0 to verify all files."
        },
        "simpleSftp.ui.hideStatusBar": {
          "type": "boolean",
          "default": false,
          "description": "Hide the transfer status bar in the bottom status bar. Default: false (show status bar)"
        },
        "simpleSftp.transfer.preservePermissions": {
          "type": "boolean",
          "default": true,
          "description": "Preserve file permissions (chmod) during transfers. Requires SFTP server support."
        },
        "simpleSftp.transfer.preserveTimestamps": {
          "type": "boolean",
          "default": true,
          "description": "Preserve file modification and access times during transfers. Requires SFTP server support."
        },
        "simpleSftp.transfer.followSymlinks": {
          "type": "boolean",
          "default": false,
          "description": "Follow symbolic links and transfer the target file/folder instead of creating symlinks. Default: false (preserve symlinks)"
        },
        "simpleSftp.parallelTransfer.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable parallel chunk-based transfer for large files. When enabled, large files will be split into chunks and transferred concurrently for faster speeds."
        },
        "simpleSftp.parallelTransfer.threshold": {
          "type": "number",
          "default": 100,
          "minimum": 10,
          "description": "Minimum file size (in MB) to trigger parallel transfer. Default: 100 MB. Files smaller than this will use single-connection transfer."
        },
        "simpleSftp.parallelTransfer.chunkSize": {
          "type": "number",
          "default": 10,
          "minimum": 1,
          "maximum": 50,
          "description": "Size of each chunk in parallel transfer (in MB). Default: 10 MB. Larger chunks may be faster but use more memory. Range: 1-50 MB."
        },
        "simpleSftp.parallelTransfer.maxConcurrent": {
          "type": "number",
          "default": 5,
          "minimum": 1,
          "maximum": 10,
          "description": "Maximum number of concurrent chunk transfers. Default: 5. Higher values may increase speed but require more connections and resources. Range: 1-10."
        },
        "simpleSftp.browser.openInEditor": {
          "type": "boolean",
          "default": false,
          "description": "Open file browser in editor area instead of panel area. When enabled, allows opening multiple browser instances for different hosts."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "node esbuild.js --production",
    "compile": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vitest run",
    "test:unit": "vitest run --exclude **/integration-tests/**",
    "test:integration": "vitest run src/integration-tests",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepackage": "npm run test:all",
    "package": "vsce package",
    "prepublish": "npm run test:all",
    "publish": "vsce publish"
  },
